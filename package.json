{
  "name": "pipeline",
  "private": true,
  "type": "module",
  "workspaces": [
    "workers",
    "localMode"
  ],
  "scripts": {
    "app:build": "node ./app/adapters.js && react-router build",
    "app:dev": "node ./app/adapters.js && cross-env NODE_ENV=development tsx watch ./server.ts",
    "app:prod": "PORT=5173 tsx ./server.ts",
    "workers:dev": "yarn workspace pipeline-worker dev",
    "workers:prod": "yarn workspace pipeline-worker prod",
    "local:redis": "yarn workspace localMode redis",
    "local:start": "./localMode/bin/start-all.js",
    "seeds": "tsx -r dotenv/config ./seeds/index.ts",
    "migration:generate": "./bin/generateMigration.js",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:typecheck": "tsc -p tsconfig.test.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
